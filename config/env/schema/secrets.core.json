{
    "version": "1.0.0",
    "secrets": [
        {
            "name": "GITHUB_TOKEN",
            "required": false,
            "description": "GitHub API token for agent operations",
            "sources": [
                "GitHub Secrets",
                "Codespaces auto-injected"
            ]
        },
        {
            "name": "NODE_ENV",
            "required": true,
            "description": "Runtime environment (development|staging|production)",
            "sources": [
                ".env",
                "agents/.env.agent.local"
            ]
        },
        {
            "name": "HIGHLIGHT_PROJECT_ID",
            "required": false,
            "description": "Highlight.io project identifier for observability",
            "sources": [
                ".env.agent.local",
                "GitHub Secrets",
                "Codespaces Secrets"
            ]
        }
    ]
}

