{
  "version": "1.0.0",
  "secrets": [
    {
      "name": "GITHUB_TOKEN",
      "required": false,
      "description": "GitHub API token for agent operations",
      "sources": ["GitHub Secrets", "Codespaces auto-injected"]
    },
    {
      "name": "NODE_ENV",
      "required": true,
      "description": "Runtime environment (development|staging|production)",
      "sources": [".env", "agents/.env.agent.local"]
    },
    {
      "name": "HIGHLIGHT_PROJECT_ID",
      "required": false,
      "description": "Highlight.io project identifier for observability",
      "sources": [".env.agent.local", "GitHub Secrets", "Codespaces Secrets"]
    },
    {
      "name": "LANGFUSE_SECRET_KEY",
      "required": true,
      "description": "Langfuse server key for tracing",
      "sources": ["config/env/.env", "GitHub Secrets"]
    },
    {
      "name": "LANGFUSE_PUBLIC_KEY",
      "required": true,
      "description": "Langfuse public key for tracing",
      "sources": ["config/env/.env", "GitHub Secrets"]
    },
    {
      "name": "GRADIENT_API_KEY",
      "required": true,
      "description": "DigitalOcean Gradient API key",
      "sources": ["config/env/.env", "GitHub Secrets"]
    },
    {
      "name": "GRADIENT_BASE_URL",
      "required": false,
      "description": "Gradient inference endpoint (defaults to https://api.digitalocean.com/v2/ai)",
      "sources": ["config/env/.env"]
    },
    {
      "name": "GRADIENT_MODEL",
      "required": true,
      "description": "Default Gradient model for the stack",
      "sources": ["config/env/.env"]
    },
    {
      "name": "GRADIENT_MODEL_ACCESS_KEY",
      "required": false,
      "description": "Optional access key for fine-tuned Gradient models",
      "sources": ["config/env/.env", "GitHub Secrets"]
    }
  ]
}
